{
	"compilerOptions": {
	  "target": "es2017",
	  "lib": ["dom", "dom.iterable", "esnext"],
	  "allowJs": false,
	  "skipLibCheck": false,
	  "strict": true,
	  "noImplicitAny": true,
	  "noImplicitThis": true,
	  "alwaysStrict": true,
	  "strictNullChecks": true,
	  "forceConsistentCasingInFileNames": true,
	  "noUnusedLocals": true,
	  "noUnusedParameters": true,
	  "noFallthroughCasesInSwitch": true,
	  "noEmit": true,
	  "esModuleInterop": true,
	  "module": "esnext",
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "isolatedModules": false,
	  "jsx": "preserve",
	  "incremental": true,
	  "plugins": [
		{
		  "name": "next"
		}
	  ],
	  "paths": {
		"@/*": ["./*"]
	  }
	},
	"include": [
	  "next-env.d.ts",
	  "**/*.ts",
	  "**/*.tsx",
	  ".next/types/**/*.ts",
	  "scripts/**/*.js"
	],
	"exclude": ["node_modules"]
  }
  